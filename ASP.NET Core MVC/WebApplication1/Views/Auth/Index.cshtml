yetkilendirme sayfası
<br />
Tarih: @DateTime.Now
<br />
Kısa Tarih : @gununTarihi

<br />
<h2>Günler</h2>
<ul>
    @foreach (var item in gunler)
    {
        <li>@item</li>
    }
    
</ul>

<br />
<br />
<h2>Öğrenci Listesi</h2>
<br />

<table class="table table-hover">
    <tr>
        <td>Id</td>
        <td>Adı</td>
        <td>Soyadı</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Adi</td>
            <td>@item.Soyadi</td>
        </tr>
    }
</table>