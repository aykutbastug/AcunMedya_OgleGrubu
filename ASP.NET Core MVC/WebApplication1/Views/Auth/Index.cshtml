@{
    string gununTarihi = "";
    gununTarihi = DateTime.Now.ToString("dd.MM.yyyy");


    string[] gunler = new string[] { "Pazartesi", "Salı", "Çarşamba", "Perşembe", "Cuma", "Cumartesi", "Pazar" };

    
}

